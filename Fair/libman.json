{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.3.1",
      "destination": "wwwroot/lib/twitter-bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.min.css"
      ]
    },
    {
      "library": "dropzone@5.5.1",
      "destination": "wwwroot/lib/dropzone/",
      "files": [
        "min/dropzone.min.js",
        "min/basic.min.css"
      ]
    },
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js"
      ]
    },
    {
      "library": "popper.js@1.15.0",
      "destination": "wwwroot/lib/popper.js/",
      "files": [
        "umd/popper.min.js"
      ]
    },
    {
      "library": "summernote@0.8.12",
      "destination": "wwwroot/lib/summernote/",
      "files": [
        "font/summernote.eot",
        "summernote-bs4.css",
        "summernote-bs4.min.js",
        "font/summernote.ttf",
        "font/summernote.woff"
      ]
    }
  ]
}